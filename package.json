    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-nginx": "^1.0.3",
    "prettier-plugin-svelte": "^3.2.6",
    "readdirp": "^3.6.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-mdx": "^3.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.1",
    "vfile-reporter": "^8.0.0",
    "vitest": "^3.0.7",
    "yaml": "^2.7.0",
    "zod": "^3.24.2",
    "zod-validation-error": "^3.4.0"
