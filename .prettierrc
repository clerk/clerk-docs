{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "bracketSpacing": true,
  "plugins": [
    "prettier-plugin-astro",
    "prettier-plugin-nginx",
    "prettier-plugin-svelte",
    "./prettier-mdx.mjs"
  ],
  "overrides": [
    {
      "files": ["*.mdx"],
      "options": {
        "parser": "mdx-custom"
      }
    }
  ]
}
