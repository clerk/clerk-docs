{
    "overrides": [
        {
            "files": ["*.mdx", "*.md"],
            "extends": ["plugin:mdx/recommended"],
            "settings": {
                "mdx/code-blocks": true
            }
        },
        {
            "files": ["*.js", "*.jsx", "*.ts", "*.tsx"],
            "extends": [
                "next/core-web-vitals",
                "plugin:prettier/recommended"
            ],
            "plugins": ["prettier"]
        }
    ]
}
